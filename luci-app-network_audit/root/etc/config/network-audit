config network-audit 'general'
    option enabled '0'
    option log_level 'info'
    option log_size '10'
    list interfaces ''
    list protocols 'tcp udp'
    option port_monitor '0'
    list suspicious_ports ''
    option rate_limit '0'
    option max_connections '100'
    option alert_email ''

config network-audit 'rules'
    list block_countries ''
    list whitelist_ips ''
    list blacklist_ips ''
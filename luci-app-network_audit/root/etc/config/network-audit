config general 'general'
    option enabled '0'
    option log_traffic '1'
    option log_level '2'
    option log_size '10'
    option real_time_monitor '1'
    option monitor_interval '5'
    list exclude_ips ''
    list exclude_ports ''

config traffic 'traffic'
    option monitor_all '1'
    list interfaces 'lan'
    list interfaces 'wan'
    option track_bandwidth '1'
    option bandwidth_history '24'
    option alert_threshold '0'
    option threshold_mbps '100'
    option track_protocols '1'
    option track_countries '0'

config access 'access'
    option enable_filtering '0'
    option block_by_default '0'

config logs 'logs'
    option enable_logging '1'
    option log_file '/var/log/network-audit.log'
    option log_connections '1'
    option log_dropped '1'
    option log_bandwidth '0'
    option log_interval '5'
    option syslog '0'
    option log_rotate '1'
    option keep_logs '7'